    <script>
        var questions = document.querySelectorAll('.content p');

        questions.forEach(function(question, index) {
            // Đặt id cho mỗi phần tử p là "question" + số thứ tự
            question.id = "question" + (index + 1);
        });

        questions.forEach(function(question) {
            // Lấy id của mỗi phần tử p
            var questionId = question.id;

            // Lấy phần tử p tương ứng với id
            var questionElement = document.getElementById(questionId);

            // Tạo một nút "Copy"
            var copyButton = document.createElement("button");
            copyButton.textContent = "Sao chép";
            copyButton.className = "copy-button";

            // Tạo một hàm onClick để sao chép nội dung khi nút được nhấp vào
            copyButton.onclick = function() {
                copyText(questionId, copyButton);
            };

            // Thêm nút "Copy" vào sau phần tử p
            questionElement.parentNode.insertBefore(copyButton, questionElement.nextSibling);
        });

        // Hàm sao chép nội dung
        function copyText(elementId, button) {
            var text = document.getElementById(elementId).innerText;
            var textarea = document.createElement("textarea");
            textarea.value = text;
            document.body.appendChild(textarea);
            textarea.select();
            document.execCommand("copy");
            document.body.removeChild(textarea);
            button.textContent = "Đã sao chép";

            // Sau 3 giây, chuyển nút đã sao chép về nút sao chép bình thường
            setTimeout(function() {
                button.textContent = "Sao chép";
                button.disabled = false;
            }, 3000);
        }
    </script>

